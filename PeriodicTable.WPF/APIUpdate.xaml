<controls:Window x:Class="PeriodicTable.WPF.APIUpdate"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PeriodicTable.WPF"
        xmlns:controls="clr-namespace:Modscleo4.WPFUI.Controls;assembly=Modscleo4.WPFUI"
        mc:Ignorable="d"
        Title="APIUpdate" Height="350" Width="600" Loaded="Window_Loaded">
    <Grid>
        <Label Foreground="#FFD5D5D5" Content="Updating data from API..." HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <ProgressBar HorizontalAlignment="Left" Height="26" Margin="158,10,0,0" VerticalAlignment="Top" Width="424" IsIndeterminate="True"/>

    </Grid>
</controls:Window>
